<?xml version="1.0" encoding="UTF-8"?>

<iridaWorkflow>
	<id>0e8738e6-2aeb-4627-9d6e-15ae32f21c44</id>
	<name>bio_hansel</name>
	<version>0.1</version>
	<analysisType>salmonella-subtyping</analysisType>
	<inputs>
		<sequenceReadsPaired>sequence_reads_paired</sequenceReadsPaired>
		<reference>reference</reference>
		<requiresSingleSample>false</requiresSingleSample>
	</inputs>
	<parameters>
		<parameter name="type_of_scheme" defaultValue="heidelberg">
			<toolParameter
				toolId="toolshed.g2.bx.psu.edu/repos/nml/bio_hansel/bio_hansel/0.1.3"
				parameterName="type_of_scheme" />
		</parameter>
		<parameter name="kmer_min" defaultValue="10">
			<toolParameter
				toolId="toolshed.g2.bx.psu.edu/repos/nml/bio_hansel/bio_hansel/0.1.3"
				parameterName="kmer_min" />
		</parameter>
		<parameter name="kmer_max" defaultValue="200">
			<toolParameter
				toolId="toolshed.g2.bx.psu.edu/repos/nml/bio_hansel/bio_hansel/0.1.3"
				parameterName="kmer_max" />
		</parameter>
		<parameter name="desired_coverage" defaultValue="50">
			<toolParameter
				toolId="toolshed.g2.bx.psu.edu/repos/nml/seqtk_nml/seqtk_nml_sample/1.0.1"
				parameterName="coverage" />
		</parameter>
	</parameters>
	<outputs>
		<output name="hansel_results" fileName="hansel_results.tab" />
		<output name="hansel_match_results" fileName="hansel_match_results.tab" />
	</outputs>
	<toolRepositories>
		<repository>
			<name>bio_hansel</name>
			<owner>nml</owner>
			<url>https://toolshed.g2.bx.psu.edu/</url>
			<revision>09ebaa5192ab</revision>
		</repository>
		<repository>
			<name>seqtk_nml</name>
			<owner>nml</owner>
			<url>https://toolshed.g2.bx.psu.edu/</url>
			<revision>f49992c79fe4</revision>
		</repository>
		<repository>
			<name>combine_tabular_collection</name>
			<owner>phil</owner>
			<url>http://galaxy-toolshed.corefacility.ca/</url>
			<revision>2480c36b4891</revision>
		</repository>
		<repository>
			<name>collapse_collections</name>
			<owner>nml</owner>
			<url>https://toolshed.g2.bx.psu.edu/</url>
			<revision>25136a2b0cfe</revision>
		</repository>
	</toolRepositories>
</iridaWorkflow>